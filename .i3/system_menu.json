{
    "Lock": "systemctl --user start i3lock.service",
    "Suspend": "systemctl suspend",
    "Reboot": "systemctl reboot",
    "Poweroff": "systemctl poweroff",
    "Keyboard": {
        "Standard": "~/.i3/keyboard_standard.sh",
        "Enhanced": "~/.i3/keyboard_enhanced.sh"
    },
    "Monitors": {
        "Clone": "ls",
        "Work setup": "xrandr --output DP1-1 --mode 1920x1080 --output eDP1 --mode 1368x768 --left-of DP1-1",
        "Laptop only": "xrandr --output DP1-1 --off --output eDP1 --mode 1600x900"
    }
}
